import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter a single charachter");
        char a = sc.next().charAt(0);
        
        int asci= a;
        
        
        System.out.println("Ascii value of "+ a + " is " +asci );
            
        
        
    }
}
